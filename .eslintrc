{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "semi": [2, "never"],
    "comma-dangle": 0,
    "no-console": 1,
    "no-debugger": 1,
    "no-underscore-dangle": 0,
    "no-alert": 0,
    "strict": 0,
    // http://eslint.org/docs/rules/no-shadow
    "no-shadow ": 0,
    // indent 2
    "indent": [2, 2],
    // http://eslint.org/docs/rules/brace-style
    "brace-style": 0,
    // all CamelCase
    "camelcase": [2, {"properties": "never"}],
    "eol-last": 0,
    "no-trailing-spaces": 0,
    "quotes": 0,
    // not var
    "no-var": 2,
    "no-unused-vars":0
  }
}
